create table produtos (
	id integer auto_increment primary key,
	nome varchar(255),
	preco decimal(10,2),
	descricao text
);
insert into produtos values (1, 'arroz', '3,5');
insert into produtos values (2, 'feijão', '4,5');
insert into produtos values (3, 'macarrão', '2,3');

create table categoria (
	id integer auto_increment primary key,
	nome varchar(255)
);
insert into categoria (nome) values ("esportes"), ("escolar"), ("mobilidade");